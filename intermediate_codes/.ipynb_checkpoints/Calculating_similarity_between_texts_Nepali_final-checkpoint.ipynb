   "source": [
    "### Jacard Similairty"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 110,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_jaccard_sim(token1, token2): \n",
    "    a = set(token1)\n",
    "    b = set(token2)\n",
    "    c = a.intersection(b)\n",
    "    return float(len(c)) / (len(a) + len(b) - len(c))"
   ]
